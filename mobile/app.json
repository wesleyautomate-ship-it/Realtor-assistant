{
  "expo": {
    "name": "PropertyPro Mobile",
    "slug": "propertypro-mobile",
    "version": "0.1.0",
    "orientation": "portrait",
    "scheme": "propertypro",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "API_BASE_URL": "http://localhost:8001",
      "AI_PROVIDER": "openai", 
      "OPENAI_API_KEY": "",
      "GOOGLE_API_KEY": ""
    }
  }
}
